{
  "manifest_version": 3,
  "name": "browser",
  "version": "0.6.0",
  "description": "browser",
  "author": "test",
  "devtools_page": "index.html",
  "action": {
    "default_icon": {
      "16": "assets/icon-16.png",
      "19": "assets/icon-19.png",
      "38": "assets/icon-38.png",
      "128": "assets/icon-128.png"
    },
    "default_title": "browser"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "38": "assets/icon-38.png",
    "128": "assets/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "browser.umd.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "chrome://favicon/"
  ]
}
